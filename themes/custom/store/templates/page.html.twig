            {{ page.content }}
            {% endblock %}
        </div>
        <div class="col-md-2 right_sidebar">
            {{ page.right_sidebar }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-12 footer">
            {{ page.footer }}
        </div>
    </div>
